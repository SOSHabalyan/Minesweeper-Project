{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=minesweeper.db"
  },
  "Database": {
    "Provider": "SQLite",
    "EnableRetryOnFailure": true,
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}